{
    "codeLens.enable": false,
    "codeLens.separator": "",
    "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
    "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
    "typescript.preferences.importModuleSpecifier": "relative",
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
    "prettier.disableSuccessMessage": true,
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "cSpell.enabled": true,
    "cSpellExt.enableDictionaries": ["portuguese-brazilian"],
    "cSpell.language": "en,pt,pt_BR",
    "yaml.trace.server": "verbose",
    "yaml.schemas": {
        "kubernetes": "/*.yaml"
    },
    "diagnostic.enableHighlightLineNumber": false,
    "diagnostic.maxWindowHeight": 16,
    "diagnostic.errorSign": "",
    "diagnostic.warningSign": "",
    "diagnostic.infoSign": "",
    "git.addedSign.text": "",
    "git.removedSign.text": "",
    "git.changedSign.text": "",
    "git.changeRemovedSign.text": "",
    "git.addedSign.hlGroup": "GitAddedSign",
    "git.removedSign.hlGroup": "GitRemovedSign",
    "git.changedSign.hlGroup": "GitChangedSign",
    "git.changeRemovedSign.hlGroup": "GitChangedSign",
    "git.addGBlameToVirtualText": false,
    "git.addGBlameToBufferVar": true,
    "cSpell.userWords": [
      "Actioner",
      "DDTHH",
      "Decentramall",
      "Emel",
      "INFURA",
      "Jonny",
      "Metamask",
      "ORBITDB",
      "Orgs",
      "Unmount",
      "autorenew",
      "backoff",
      "bindgen",
      "bytecode",
      "decorrelated",
      "deletable",
      "emmiter",
      "ethereum",
      "focusbc",
      "gcloud",
      "hardcoded",
      "helloworld",
      "ipfs",
      "kcubectl",
      "kubectl",
      "mapify",
      "promisify",
      "upsert",
      "uuidv",
      "wasm",
      "watchings"
    ]
}
