{
    "editor.formatOnSave": true,
    "editor.renderIndentGuides": true,
    "editor.lineNumbers": "relative",
    "keyboard.dispatch": "keyCode",
    "editor.wordWrap": "on",
    "prettier.jsxSingleQuote": true,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "all",
