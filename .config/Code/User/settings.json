{
    "telemetry.enableTelemetry": false,
    "explorer.autoReveal": false,
    "workbench.colorTheme": "Apprentice Bright",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontFamily": "SauceCodePro Nerd Font Mono",
    "editor.fontFamily": "'SauceCodePro Nerd Font Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.formatOnSave": true,
    "editor.renderIndentGuides": true,
    "editor.lineNumbers": "relative",
    "editor.minimap.maxColumn": 80,
    "editor.cursorWidth": 80,
    "editor.wordWrap": "on",
    "editor.codeLens": false,
    "editor.renderWhitespace": "boundary",
    "editor.cursorStyle": "line-thin",
    "editor.cursorBlinking": "solid",
    "editor.suggestSelection": "first",
    "editor.tokenColorCustomizations": {
        "functions": "#d7d787"
    },
    "keyboard.dispatch": "keyCode",
    "prettier.jsxSingleQuote": true,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "all",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vscode-neovim.neovimInitPath": " ~/.config/nvim/init.vim",
    "vscode-neovim.neovimPath": "/usr/local/bin/nvim",
    "window.zoomLevel": 1
}
